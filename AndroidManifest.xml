<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.ypid.complexalarm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/cm_app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ActivityAlarmList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityAlarmAdd"
            android:label="@string/add_alarm" >
        </activity>
        <activity
            android:name=".OpeningHoursWebView"
            android:label="@string/opening_hours_web_view" >
        </activity>
    </application>

</manifest>
